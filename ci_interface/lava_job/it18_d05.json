{    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "metadata": {
                "boot.type": "ACPI",
                "image.type": "kernel-ci",
                "image.url": "http://localhost:8083/debug-plinth-ci/debug-it18-4.16.0+_+20180314",
                "kernel.defconfig": "arm64-debug",
                "kernel.endian": "little",
                "kernel.tree": "plinth",
                "kernel.version": "debug-it18-4.16.0+_+20180314",
                "platform.fastboot": "false",
                "test.plan": "sas roce pcie hns debug test"
            },
            "parameters": {
                "bootloadertype": "ipxe",
                "dtb": "http://localhost:8083/debug-plinth-ci/debug-it18-4.16.0+_+20180314/arm64-debug/hip07-d05.dtb",
                "kernel": "http://localhost:8083/debug-plinth-ci/debug-it18-4.16.0+_+20180314/arm64-debug/Image",
                "target_type": "ubuntu"
            }
        },
        {
            "command": "boot_linaro_image",
            "parameters": {
                "options": [
                    "boot_cmds=boot_cmds_disc_acpi"
                ]
            }
        },

        {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_repos": [
                    {
                        "git-repo": "https://github.com/Luojiaxing1991/ci_autotest.git",
			"parameters":{
			     "Maintainer": "Luojiaxing"
			},
			"testdef": "xge_autotest/xge_autotest.yaml"
                    }
                ],
                "timeout": 172800
            }
        },
        {
            "command": "boot_linaro_image",
            "parameters": {
                "options": [
                    "boot_cmds=expect 'Move Highlight',sendcontrol '[',sendline '[B',sendcontrol '[',sendline '[B',sendcontrol 'M',sendcontrol M,expect grub>,sendline 'set root=(tftp,192.168.3.100)',expect grub>,sendline 'linux /plinth/hip07-d05_159/.Image rdinit=/init console=ttyAMA0,115200 earlycon=sdsa-uart,mmio,0x602b0000 root=/dev/sda2 rootfstype=ext4 rw ip=:::::eth0:dhcp acpi=force pcie_aspm=off',expect grub>,sendline boot,expect 'login',sendline root,expect 'Password',sendline 'root'"
                ]
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "http://localhost:8089/RPC2/",
                "stream": "/anonymous/plinth/"
            }
        }
    ],
    "device_type": "hip07-d05",
    "job_name": "It18 Test",
    "logging_level": "DEBUG",
    "priority": "high",
    "timeout": 172800
}

